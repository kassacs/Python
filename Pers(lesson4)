subjects_list = ["хоббит", "Федор", "Сумкин",        # имя списка  с маленькой буквы,  во мн. числе
                'рост', 120, "вес", 40,              # перенесена длинная строка, добавлены пробелы после запятых
                "эльф", "Арвен", 'рост',             # перенесена длинная строка, добавлены пробелы после запятых
                 188, "вес", 25,                     # перенесена длинная строка, добавлены пробелы после запятых
                "человек", "Боромир",               # перенесена длинная строка, добавлены пробелы после запятых
                'рост', 179, "вес", 105]            # перенесена длинная строка
subjects_dict = {("хоббит", "Федор", "Сумкин"):     # имя словаря  с маленькой буквы , во мн. числе
                {"рост": 120, "вес":40},            # перенесена длинная строка, добавлены пробелы после запятых
                ('эльф', "Арвен"):                  # перенесена длинная строка, добавлены пробелы после запятых, двоеточий
                {"рост": 188, "вес": 25},           # перенесена длинная строка, добавлены пробелы после запятых, двоеточий
                ("человек", "Боромир"):             # перенесена длинная строка, добавлены пробелы после запятых, двоеточий
                {"рост": 179, "вес": 105}           # перенесена длинная строка, добавлены пробелы после запятых, двоеточий
print(subjeсts_dict["хоббит", "Федор", "Сумкин"])   # добавлены пробелы после запятых, двоеточий
print(subjeсts_list[13:])
for subj, descr in subjects_dict.items():
    if "Федор" in subj:
        print(descr)
for subj in subjects_list:                                              
    if subj == "Арвен":                             # добавлен пробел рядом с оператором
        print(subjeсts_list[subjects_list.index("Арвен"):])
keys, value=input().split()                         # исправлено имя переменной с транслита на английское слово
for sub, desc in subjeсts_dict.items(): 
    for subj, descr in subjects_dict[sub].items():
        if keys == subj and int(value) == descr:
            print(sub)
                                         
